-i ~/2018/Non_filtered_Analysis/Qcore_diversity_more/openS_wTree/arare_max38882/alpha_div_collated/PD_whole_tree.txt \
-m ~/map_complete.tsv \
-o ~/2018/Non_filtered_Analysis/Qcore_diversity_more/openS_wTree/arare_max38882/Compare_Alpha/Geography \
