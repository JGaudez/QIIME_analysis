#!/bin/bash
#SBATCH -t 1:00:00
#SBATCH -n 24
#SBATCH -p short

#Load modules to run python
module load eb
module load Miniconda2
source deactivate
# loading virtualenv
echo "loading virtualenv"
source activate qiime1
# setting temporary directory
export TMPDIR=~/qiime_tmp
# analysing alpha diversity
echo "Running alpha-diversity on non-subsampled OTUs, Q NF Silva open"
time alpha_diversity.py -t ~/2018/Qotus/openS/rep_set.tre \
-i ~/2018/Qotus/openS/otu_table_mc2_w_tax_no_pynast_failures.biom \
-o ~/2018/Non_filtered_Analysis/alpha_diversity_all_NFopenSQ.txt 

source deactivate
